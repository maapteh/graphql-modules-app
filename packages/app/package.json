{
  "name": "graphql-app",
  "version": "0.0.3",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "maapteh/graphql-modules-app/packages/graphql-app"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "scripts": {
    "prepare": "yarn build",
    "dev": "next -p 4001",
    "build": "next build",
    "start": "PORT=$PORT NODE_ENV=production node --optimize_for_size --max_old_space_size=920 --gc_interval=100 server.js",
    "start:local": "NODE_ENV=production next start -p 4001",
    "test": "echo \"No tests yet\" && exit 0",
    "test:coverageNOTESTSYET": "jest --verbose --coverage",
    "test:ci": "yarn run coverage -- --ci --maxWorkers=2 --reporters=default --reporters=jest-junit",
    "lint": "tslint -c \"../../config/tslint.json\" -p tsconfig.json src/*.ts",
    "lint-staged": "lint-staged",
    "clean": "find . -name \"node_modules\" -exec rm -rf '{}' + && find . -name \"dist\" -exec rm -rf '{}' +",
    "generate-components": "gql-gen"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@apollo/react-ssr": "^3.0.1",
    "@types/react": "^16.8.6",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.8",
    "apollo-link-batch-http": "^1.2.8",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "apollo-utilities": "^1.3.2",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "lint-staged": "^8.1.4",
    "next": "9.0.5",
    "node-sass": "^4.9.3",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-apollo": "^2.5.1",
    "react-dom": "^16.8.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.7.0",
    "@graphql-codegen/cli": "^1.7.0",
    "@graphql-codegen/fragment-matcher": "^1.7.0",
    "@graphql-codegen/time": "^1.7.0",
    "@graphql-codegen/typescript-operations": "^1.7.0",
    "@graphql-codegen/typescript-react-apollo": "^1.7.0",
    "@types/graphql": "^14.5.0",
    "dts-css-modules-loader": "^1.0.1",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "webpack": "^4.29.6"
  },
  "lint-staged": {
    "**/{components,pages}/**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
