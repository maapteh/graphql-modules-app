{
  "name": "graphql-app",
  "version": "0.0.2",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "maapteh/graphql-modules-app/packages/graphql-app"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "scripts": {
    "prepare": "yarn build",
    "dev": "PORT=4001 node server.js",
    "build": "next build",
    "start": "PORT=$PORT NODE_ENV=production node server.js",
    "start:local": "PORT=4001 NODE_ENV=production node server.js",
    "test": "jest --verbose",
    "test:coverageNOTESTSYET": "jest --verbose --coverage",
    "test:ci": "yarn run coverage -- --ci --maxWorkers=2 --reporters=default --reporters=jest-junit",
    "lint": "tslint -c \"../../config/tslint.json\" -p tsconfig.json src/*.ts",
    "lint-staged": "lint-staged",
    "clean": "find . -name \"node_modules\" -exec rm -rf '{}' + && find . -name \"dist\" -exec rm -rf '{}' +",
    "generate-components": "gql-gen",
    "build-introspection": "node internals/introspection.js"
  },
  "dependencies": {
    "@types/next": "^7.0.5",
    "@types/react": "^16.7.13",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-cache-inmemory": "^1.3.11",
    "apollo-client": "^2.4.7",
    "apollo-link": "^1.2.4",
    "apollo-link-batch-http": "1.2.4",
    "apollo-link-error": "^1.1.2",
    "apollo-link-http": "^1.5.7",
    "express": "^4.16.4",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "isomorphic-unfetch": "^3.0.0",
    "lint-staged": "^8.1.0",
    "next": "^7.0.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-apollo": "^2.3.2",
    "react-dom": "^16.6.3",
    "typescript": "^3.2.1"
  },
  "devDependencies": {
    "graphql-code-generator": "^0.14.5",
    "graphql-codegen-add": "0.14.5",
    "graphql-codegen-time": "0.14.5",
    "graphql-codegen-typescript-client": "^0.14.5",
    "graphql-codegen-typescript-react-apollo": "0.14.5",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.15.0",
    "webpack": "^4.27.1"
  },
  "lint-staged": {
    "**/{components,pages}/**/*.{ts,tsx,graphql}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
